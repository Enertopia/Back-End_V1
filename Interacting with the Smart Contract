const contractABI = [/* ... ABI ... */];
const contractAddress = "0x..."; // Deployed contract address

const rinContract = new web3.eth.Contract(contractABI, contractAddress);

async function createRIN(data) {
    const accounts = await web3.eth.getAccounts();
    if (accounts.length === 0) {
        console.error("No accessible accounts");
        return;
    }

    const result = await rinContract.methods.createRIN(/* parameters */).send({ from: accounts[0] });
    console.log(result);
}
